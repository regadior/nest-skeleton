{
   "name": "nest-skeleton",
   "version": "0.0.1",
   "description": "",
   "author": "",
   "private": false,
   "license": "UNLICENSED",
   "engines": {
      "node": ">=20.6.0",
      "npm": ">=9.8.1"
   },
   "scripts": {
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "ts-node src/index.ts",
      "start:dev": "nest start --tsc --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "check:types": "tsc --pretty --noEmit",
      "check:format": "prettier --check .",
      "check:lint": "eslint . --ext .js,.ts --color",
      "check:spelling": "cspell \"{README.md,TODO.md,.github/*.md,src/**/*.ts}\"",
      "prepare": "husky install",
      "typeorm": "typeorm-ts-node-commonjs",
      "commit": "cz"
   },
   "dependencies": {
      "@nestjs/common": "^10.0.0",
      "@nestjs/config": "^3.0.0",
      "@nestjs/core": "^10.0.0",
      "@nestjs/platform-express": "^10.0.0",
      "@nestjs/swagger": "^7.1.1",
      "@nestjs/typeorm": "^10.0.0",
      "@prisma/client": "^5.6.0",
      "@types/express": "^4.17.17",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.0",
      "cz": "^1.8.2",
      "dotenv": "^16.3.1",
      "dotenv-defaults": "^5.0.2",
      "dotenv-expand": "^10.0.0",
      "express": "^4.18.2",
      "http-status-codes": "^2.2.0",
      "mysql2": "^3.6.2",
      "nestjs-pino": "^3.3.0",
      "node-emoji": "^2.1.0",
      "pg": "^8.4.0",
      "pino-http": "^8.3.3",
      "pino-pretty": "^10.0.1",
      "pretty-quick": "^3.1.3",
      "reflect-metadata": "^0.1.13",
      "rxjs": "^7.8.1",
      "socket.io": "^4.7.1",
      "swagger": "^0.7.5",
      "typeorm": "0.3.17",
      "winston": "^3.10.0"
   },
   "devDependencies": {
      "@nestjs/cli": "^10.0.0",
      "@nestjs/schematics": "^10.0.0",
      "@nestjs/testing": "^10.0.0",
      "@types/jest": "^29.5.2",
      "@types/node": "^16.11.10",
      "@types/supertest": "^2.0.12",
      "@typescript-eslint/eslint-plugin": "^6.0.0",
      "@typescript-eslint/parser": "^6.0.0",
      "cspell": "^7.3.8",
      "cz-conventional-changelog": "^3.3.0",
      "eslint": "^8.42.0",
      "eslint-config-airbnb-typescript": "^17.1.0",
      "eslint-config-prettier": "^9.0.0",
      "eslint-import-resolver-typescript": "^3.6.0",
      "eslint-plugin-deprecation": "^1.5.0",
      "eslint-plugin-eslint-comments": "^3.2.0",
      "eslint-plugin-jest": "^27.2.3",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-optimize-regex": "^1.2.1",
      "eslint-plugin-prettier": "^5.0.0",
      "eslint-plugin-promise": "^6.1.1",
      "eslint-plugin-security": "^1.7.1",
      "eslint-plugin-simple-import-sort": "^10.0.0",
      "eslint-plugin-sonarjs": "^0.20.0",
      "eslint-plugin-unicorn": "^48.0.1",
      "eslint-plugin-unused-imports": "^3.0.0",
      "husky": "^8.0.3",
      "jest": "^29.5.0",
      "lint-staged": "^13.3.0",
      "prettier": "^3.0.3",
      "source-map-support": "^0.5.21",
      "supertest": "^6.3.3",
      "ts-jest": "^29.1.0",
      "ts-loader": "^9.4.3",
      "ts-node": "10.7.0",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.2.2"
   },
   "prisma": {
      "schema": "src/infrastructure/common/persistence/prisma/schema.prisma",
      "seed": "ts-node src/infrastructure/common/persistence/prisma/seed.ts"
    },
   "lint-staged": {
      "*.js": "eslint --cache --fix"
   },
   "config": {
      "commitizen": {
         "path": "git-cz"
      }
   }
}
